      {
        "language": "sql",
        "path": "./snippets/sql.json"
      },
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "html",
          "jade",
          "pug"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "css",
          "scss",
