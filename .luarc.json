{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": ["sphynx", "vim", "describe", "it", "before_each", "after_each", "pending"],
    "disable": ["missing-fields"]
  },
  "completion": {
    "callSnippet": "Replace",
    "keywordSnippet": "Replace"
  },
  "hint": {
    "enable": true,
    "setType": true
  },
  "telemetry": {
    "enable": false
  }
}
